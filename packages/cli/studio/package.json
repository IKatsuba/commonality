{
  "name": "@commonalityco/studio",
  "version": "0.1.5",
  "main": "dist/start.js",
  "types": "dist/start.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm run dev:script & pnpm run dev:app",
    "dev:script": "tsup-node --config scripts/tsup.config.ts --watch",
    "dev:app": "NODE_ENV=development node dist/dev-start.js",
    "build:script": "tsup-node --config scripts/tsup.config.ts",
    "build:app": "next build",
    "build": "pnpm run build:script && pnpm run build:app",
    "analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node dist/dev-start.js",
    "lint": "next lint",
    "e2e:run-script": "node dist/dev-start.js",
    "e2e": "pnpm run build:script &&  start-server-and-test e2e:run-script http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "pnpm run build:script && start-server-and-test e2e:run-script http://localhost:3000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@commonalityco/data-codeowners": "*",
    "@commonalityco/data-documents": "*",
    "@commonalityco/data-packages": "*",
    "@commonalityco/data-project": "*",
    "@commonalityco/data-graph": "*",
    "@commonalityco/data-tags": "*",
    "@commonalityco/data-violations": "*",
    "@commonalityco/feature-graph": "*",
    "@commonalityco/types": "*",
    "@commonalityco/ui-core": "*",
    "@commonalityco/ui-design-system": "*",
    "@commonalityco/ui-graph": "*",
    "@commonalityco/utils-core": "^0.0.2",
    "@commonalityco/utils-graph": "*",
    "@commonalityco/utils-package": "*",
    "@next/bundle-analyzer": "^13.4.12",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "@types/node": "18.14.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "18.0.11",
    "@xstate/react": "^3.2.2",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "cytoscape": "^3.25.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "execa": "5.1.1",
    "fs-extra": "^11.1.1",
    "get-port": "5.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.248.0",
    "next": "13.4.6",
    "next-themes": "^0.2.1",
    "pino": "^8.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "xstate": "^4.38.1"
  },
  "files": [
    ".next/",
    "next.config.js",
    "dist/"
  ],
  "devDependencies": {
    "@commonalityco/config-eslint": "*",
    "@commonalityco/config-tailwind": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/lodash.sortby": "^4.7.7",
    "@types/resolve": "^1.20.2",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.17.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "pino-pretty": "^10.0.1",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}

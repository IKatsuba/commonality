{
  "name": "commonality",
  "version": "0.0.26",
  "bin": {
    "commonality": "./bin.js"
  },
  "engines": {
    "node": ">=14.6"
  },
  "description": "Infinitely scalable front-end ecosystems",
  "main": "dist/commonality.cjs.js",
  "module": "dist/commonality.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/commonality.cjs.mjs",
        "default": "./dist/commonality.cjs.js"
      },
      "module": "./dist/commonality.esm.js",
      "import": "./dist/commonality.cjs.mjs",
      "default": "./dist/commonality.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/commonalityco/commonalityco",
  "bugs": "https://github.com/commonalityco/commonalityco/issues",
  "homepage": "https://commonality.co",
  "license": "MPL-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --external:@commonalityco/studio --outdir=dist --minify",
    "dev": "tsup --watch",
    "lint": "eslint ./src --fix",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@commonalityco/studio": "workspace:^",
    "@commonalityco/data-codeowners": "workspace:^",
    "@commonalityco/data-documents": "workspace:^",
    "@commonalityco/data-packages": "workspace:^",
    "@commonalityco/data-project": "workspace:^",
    "@commonalityco/data-tags": "workspace:^",
    "@commonalityco/data-violations": "workspace:^",
    "@commonalityco/utils-core": "workspace:^",
    "chalk": "4.1.2",
    "cliui": "^8.0.1",
    "commander": "^9.5.0",
    "get-port": "5.1.1",
    "got": "^11.8.6",
    "open": "^8.4.2",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@commonalityco/config-eslint": "*",
    "@commonalityco/config-tsconfig": "*",
    "@commonalityco/config-tsup": "*",
    "@commonalityco/types": "*",
    "@swc/core": "^1.3.69",
    "@types/configstore": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.19",
    "@types/update-notifier": "^6.0.4",
    "configstore": "3.1.5",
    "esbuild": "^0.19.1",
    "fs-extra": "^11.1.1",
    "ncp": "^2.0.0",
    "strip-ansi": "^7.1.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.33.0"
  }
}

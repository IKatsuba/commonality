{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build", "build"]
    },
    "test:watch": {
      "cache": false
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "type-check": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}

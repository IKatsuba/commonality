{
  "stripScopeFromPackageNames": true,
  "constraints": [
    {
      "tag": "feature",
      "allow": ["*"]
    },
    {
      "tag": "config",
      "allow": ["config"]
    },
    {
      "tag": "ui",
      "allow": ["ui", "utility", "config"]
    },
    {
      "tag": "data",
      "allow": ["data", "utility", "config"]
    },
    {
      "tag": "utility",
      "allow": ["data", "utility", "config"]
    }
  ]
}
